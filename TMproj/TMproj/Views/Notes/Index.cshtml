@model IEnumerable<TMproj.Models.Note>

@{
    ViewBag.Title = "Index";
}

<h2>Index</h2>

<p>
    @Html.ActionLink("Create New", "Create")
</p>
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.created)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.title)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.article)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.created)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.title)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.article)
        </td>
        <td>
            @Html.ActionLink("Edit", "Edit", new { id=item.noteID }) |
            @Html.ActionLink("Details", "Details", new { id=item.noteID }) |
            @Html.ActionLink("Delete", "Delete", new { id=item.noteID })
        </td>
    </tr>
}

</table>
